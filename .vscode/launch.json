    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File (Integrated Terminal)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${fileDirname}"
        },
        {
            "name": "Google App Engine application",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/main.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/src/"
        },
        {
            "name": "Google App Engine application (new source)",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src-new/main.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/src-new/"
        }
    ]
}